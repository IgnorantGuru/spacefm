#, c-format
msgid ""
"\n"
"INTERNAL COMMANDS:\n"
msgstr "\nComandos internos:\n\n"

#: ../src/cust-dialog.c:3736
#, c-format
msgid ""
"\n"
"EXAMPLE WITH COMMANDS:\n"
msgstr "\nExemplo com comandos:\n"

#: ../src/cust-dialog.c:3737
#, c-format
msgid ""
"    spacefm -g --label \"Enter some text and press Enter:\" \\\n"
"               --input \"\" set label2 %%v -- echo '# %%n = %%v' \\\n"
"               --label \\\n"
"               --button ok\n"
msgstr "    spacefm -g --label \"O texto que me apetecer e premir Enter:\" \\\n               --input \"\" set label2 %%v -- echo '# %%n = %%v' \\\n               --label \\\n               --button ok\n"

