msgid ""
"\n"
"EXAMPLE SCRIPT:\n"
msgstr "\nEXEMPEL SKRIPT:\n"

#: ../src/cust-dialog.c:3734
#, c-format
msgid ""
"    #!/bin/bash\n"
"    # This script shows a Yes/No dialog\n"
"    # Use QUOTED eval to read variables output by SpaceFM Dialog:\n"
"    eval \"`spacefm -g --label \"Are you sure?\" --button yes --button no`\"\n"
"    if [[ \"$dialog_pressed\" == \"button1\" ]]; then\n"
"        echo \"User pressed Yes - take some action\"\n"
"    else\n"
"        echo \"User did NOT press Yes - abort\"\n"
"    fi\n"
msgstr "#!/bin/bash\n# Detta skript visar en Ja/Nej dialogruta\n# Använd QUOTED eval för att läsa variabler med SpaceFM Dialog:\neval \"` spacefm -g --label \"Är du säker?\" --button yes --button no `\"\nif [[\"$ dialog_pressed\" == \"button1\"]]; then\necho \"Användaren tryckte Ja - vidta några åtgärder\"\nelse\necho \"Användaren tryckte inte Ja - abort\"\nfi\n"

